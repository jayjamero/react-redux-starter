{
  "name": "react-redux-starter",
  "version": "3.0.0",
  "description": "Simple starter pack featuring React and Redux",
  "main": "main.js",
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 10"
  ],
  "dependencies": {
    "axios": "^0.18.0",
    "prop-types": "^15.6.1",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "2.4.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^8.0.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "file-loader": "^1.1.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "jest-webpack-alias": "^3.3.3",
    "live-server": "^1.2.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.8",
    "redux-mock-store": "^1.5.1",
    "sass-loader": "^7.0.1",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.9.0",
    "webpack-cli": "^2.1.2"
  },
  "scripts": {
    "test": "jest --config=jest.config.json",
    "test:watch": "jest --watch --config=jest.config.json",
    "test:coverage": "jest --coverage",
    "dev": "webpack --config=./webpack.config.babel.js --watch --hot --progress --mode=development",
    "dev:debug": "webpack --config=./webpack.config.babel.js --watch --hot --progress --debug",
    "build": "webpack --config=./webpack.config.babel.js --progress --mode=development",
    "build:release": "webpack --config=./webpack.config.babel.js --progress --optimize-minimize --mode=production",
    "server": "cd build && live-server --port=4321 --entry-file=index.html",
    "start": "npm run build:release && npm run server"
  },
  "repository": {
    "type": "git"
  },
  "author": "Jay Jamero",
  "license": "ISC"
}
