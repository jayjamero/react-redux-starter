{
  "name": "react-redux-starter",
  "version": "2.0.0",
  "description": "Simple starter pack featuring React and Redux",
  "main": "main.js",
  "browserslist": [
    ">1%",
    "last 4 versions",
    "Firefox ESR",
    "not ie < 10"
  ],
  "dependencies": {
    "axios": "^0.17.0",
    "babel-polyfill": "^6.26.0",
    "prop-types": "^15.6.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "redux": "^3.7.2",
    "redux-promise": "^0.5.3",
    "redux-thunk": "^2.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-constant-elements": "^6.23.0",
    "babel-plugin-transform-react-inline-elements": "^6.22.0",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-polyfill": "^6.26.0",
    "babel-preset-airbnb": "2.4.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babelify": "^8.0.0",
    "clean-webpack-plugin": "^0.1.17",
    "css-loader": "^0.28.7",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.3",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^21.2.1",
    "jest-webpack-alias": "^3.3.3",
    "live-server": "^1.2.0",
    "node-sass": "^4.5.3",
    "postcss-loader": "^2.0.8",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "svg-url-loader": "^2.3.0",
    "url-loader": "^0.6.2",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.0"
  },
  "scripts": {
    "test": "jest",
    "test:coverage": "jest --coverage",
    "dev": "webpack --config=./webpack.config.babel.js --watch --hot --progress",
    "dev:debug": "webpack --config=./webpack.config.babel.js --watch --hot --progress --debug",
    "build": "webpack --config=./webpack.config.babel.js --progress",
    "build:release": "webpack --config=./webpack.config.babel.js --progress --optimize-minimize",
    "start": "cd build && live-server --port=4321 --entry-file=index.html"
  },
  "jest": {
    "setupFiles": [
      "./src/utilities/shims.util.js",
      "./src/utilities/setupTests.util.js"
    ],
    "collectCoverageFrom": [
      "**/*.{js,jsx}",
      "!**/*.{config,util}.*",
      "!**/coverage/**",
      "!**/node_modules/**",
      "!**/vendor/**",
      "!**/build/**"
    ],
    "moduleNameMapper": {
      "\\.(css|scss|sass)$": "identity-obj-proxy"
    }
  },
  "repository": {
    "type": "git"
  },
  "author": "Jay Jamero",
  "license": "ISC"
}
